issn = {0013-936X},
journal = {Environmental science {\&} technology},
keywords = {Conservation of Natural Resources,Engineering,Engineering: trends,Environmental Pollution,Environmental Pollution: prevention {\&} control,Goals,Humans,Industry,Interdisciplinary Communication,Social Conditions},
month = {dec},
