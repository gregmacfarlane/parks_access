@inproceedings{ite_rightturns,
abstract = {The delay of right-turning vehicles at intersections is an important but little-studied aspect of traffic intersection operations. Several different physical treatments can be applied to right turns, including turn lanes, turn channels, and free right-turn (RT) channels with auxiliary lanes exiting the intersection. It is believed that improvements such as these will alleviate RT delay at intersections. Yet motorists still frequently yield to oncoming vehicles in conditions that permit them proceed without delay. In order to study how drivers behave at RT channels, right-turning vehicles at two "free right-turn" channelized intersections in Utah County, Utah, were video-recorded for statistical analyses. The vehicles were observed to yield to oncoming vehicles even when no actual conflict existed. Linear and logarithmic regression models predict that each vehicle in a free RT channel experiences 2 seconds of delay for every oncoming vehicle. Existing traffic models fail to incorporate this delay into their algorithms, often leading to an underestimation of delay at RT channels. These findings are important in two major ways. First, it is recommended the Transportation Research Board and other entities consider ways to refine how RT behavior is modeled and simulated to more accurately reflect imperfect motorist understanding and behavior. Second, municipalities and departments of transportation should re-evaluate the decision to install free RT channels, especially free RT channels, because the benefits of the treatment may not be significantly greater than other RT alternatives.},
author = {Macfarlane, G.S. and Saito, M. and Schultz, G.G.},
booktitle = {Institute of Transportation Engineers Annual Meeting and Exhibit 2011},
isbn = {9781618391988},
title = {{Are free right-turn channelized intersections performing as they should}},
year = {2011}
}
@article{Cantillo2018,
abstract = {Transportation networks and systems are vulnerable to natural disasters. During disaster response operations, the degraded functionality of the system can negatively impact the affected population because disrupting relief activitiesincreases human suffering resulting from the lack of access to essential goods or services. Mathematical formulations for assessing transportation network vulnerability do not generally consider this lack of access or deprivation costs, and can lead to inappropriate strategies for humanitarian assistance. This paper proposes a transportation network vulnerability assessment model that allows identifying critical links for the development of high impact disaster response operations. The model is based on an economic analysis that considers the logistical costs of the distribution operations and the external effects derived from the delays in the provision of basic supplies (deprivation costs). The approach is particularly useful for planning resilient disaster response plans in the preparednessstage, prioritizing investment for mitigation and adaptation, and prioritizing the rehabilitation (access restoration) of the disrupted links in the response and recovery stages. In addition to numerical experiments using case study networks, the authors implemented the model to the coffee-producing region of Colombia, which was hit by an earthquake in 1999.},
author = {Cantillo, Victor and Macea, Luis F and Jaller, Miguel},
doi = {10.1007/s11067-017-9382-x},
file = {:Users/gregmacfarlane/Documents/Mendeley Desktop/2018/Cantillo, Macea, Jaller{\_}2018{\_}Assessing Vulnerability of Transportation Networks for Disaster Response Operations.pdf:pdf},
journal = {Networks and Spatial Economics},
keywords = {Accessibility,Deprivation costs,Humanitarian logistics,Resilience,Social costs,Vulnerability},
title = {{Assessing Vulnerability of Transportation Networks for Disaster Response Operations}},
url = {https://doi.org/10.1007/s11067-017-9382-x},
year = {2018}
}
