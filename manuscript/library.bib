@unpublished{Bhat2010,
abstract = {The likelihood functions of many discrete unordered-response choice models entail the evaluation of analytically-intractable integrals. As a result, such models are usually estimated using maximum simulated likelihood (MSL) techniques. Unfortunately, for many practical situations, the computational cost to ensure good asymptotic MSL estimator properties can be prohibitive and literally infeasible as the number of dimensions of integration rises. In this paper, we introduce a maximum approximated composite marginal likelihood (MACML) estimation approach that can be applied using simple optimization software for likelihood estimation. It also represents a conceptually and pedagogically simpler simulation-free procedure relative to simulation techniques, and has the advantage of reproducibility of the results and substantial computational efficiency relative to the MSL approach. The paper provides a âblueprintâ for the MACML estimation for a wide variety of cross-sectional and panel unordered response models.},
address = {Austin, Texas},
annote = {sent to Central Printing 2010.07.26},
author = {Bhat, Chandra R.},
institution = {University of Texas at Austin},
keywords = {Composite marginal likelihood,Discrete choice models,Maximum simulated likelihood,Mixed multinomial logit,Mixed multinomial probit,Normal scale mixture,Panel data,Spatial econometrics,Unordered- response models},
pages = {46},
title = {{The Maximum Approximated Composite Marginal Likelihood (MACML) Estimation of Mixed Unordered Response Choice Models}},
year = {2010}
}
@article{Ioannides2008,
abstract = {This paper contributes to the growing literature that identiï¬es and measures the impact of social context on individual economic behavior. We develop a model of housing demand with neighborhood eï¬ects and neighborhood choice. Modelling neighborhood choice is of fundamental importance in estimating and understanding endogenous and exogenous neighborhood eï¬ects. That is, to obtain unbiased estimates of neighborhood eï¬ects, it is necessary to control for non-random sorting into neighborhoods. Estimation of the model exploits a unique data set of household data that has been augmented with contextual information at two diï¬erent levels (âscalesâ) of aggregation. One is at the neighborhood cluster level, of about ten neighbors, with the data coming from a special sample of the American Housing Survey. A second level is the census tract to which these dwelling units belong. Tract-level data are available in the Summary Tape Files of the decennial Census data. We merge these two data sets by gaining access to conï¬dential data of the U.S. Bureau of the Census. Our results for the neighborhood choice model indicate that individuals prefer to live near others like themselves. Our estimates of the housing demand equation do conï¬rm that neighborhood eï¬ects are important. Neighborhood choice appears to be associated with the following diï¬erences, when it is accounted for relative to when it is not, as in earlier work. First, there is a smaller endogenous social eï¬ect: the elasticity of housing demand with respect to mean neighbors' demand is 0.6244 instead of 0.660 in earlier work. Second, there is a larger contextual eï¬ect: the elasticity of housing demand with respect to mean neighbors' income is 0.2600 instead of 0.175.},
author = {Ioannides, Y and Zabel, J},
