@article{Lee2010,
abstract = {Residential mobility and relocation choice are essential parts of integrated transportation and land use models. These decision processes have been examined and modeled individually to a great extent but there remain gaps in the literature on the underlying behaviors that connect them. Households may partly base their decision to move from or stay at a current location on the price and quality of the available alternatives. Conversely, households that are on the market for a new location may evaluate housing choices relative to their previous residence. How and the degree to which these decisions relate to each other are, however, not completely understood. This research is intended to contribute to the body of empirical evidence that will help answer these questions. It is hypothesized that residential mobility and location choice are related household decisions that can be modeled together using a two-tier hierarchical structure. This paper presents a novel nested logit (NL) model with sampling of alternatives and a proposed procedure for sampling bias correction. The model was estimated using fullinformation maximum likelihood estimation methods. The results confirm the applicability of this NL model and support similar findings from other empirical studies in the residential mobility and location choice literature.},
author = {Lee, Brian H. Y. and Waddell, Paul},
file = {:Users/gregmacfarlane/Documents/Mendeley Desktop/2010/Lee, Waddell{\_}2010{\_}Residential mobility and location choice a nested logit model with sampling of alternatives.pdf:pdf},
issn = {0049-4488},
journal = {Transportation},
keywords = {choice {\'{a}} nested logit,residential mobility {\'{a}} location,sampling of alternatives,{\'{a}}},
month = {apr},
number = {4},
pages = {587--601},
title = {{Residential mobility and location choice: a nested logit model with sampling of alternatives}},
url = {http://www.springerlink.com/index/10.1007/s11116-010-9270-4},
volume = {37},
year = {2010}
}
