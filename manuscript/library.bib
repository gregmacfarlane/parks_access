@article{Cao2007a,
abstract = {Auto ownership is a critical mediating link in the connection between the built environment and travel behavior: the built environment presumably influences auto ownership, which in turn impacts travel behavior. However, the way in which individual elements of the built environment affect auto-ownership choices is far from understood. Further, residential self-selection may confound the interaction between the built environment and auto ownership. And the absence of panel data impedes our understanding of the causal relationships. Using a survey of 1682 respondents in Northern California, we applied ordered probit and static-score modeling techniques to investigate the causal link from the built environment to auto ownership in both cross-sectional and quasi-panel contexts. Through variable selection in cross-sectional analysis, we found that individuals' attitudes regarding residential neighborhood and travel are more strongly associated with their auto-ownership decision than is the built environment per se. Specifically, when general preferences for various neighborhood traits were allowed to enter the model, they drove out from the model the (perceived) measure of the same trait for the neighborhood of current residence, a pattern suggesting that the observed correla- tion between neighborhood characteristics and auto ownership is primarily a result of self-selection. On the other hand, the quasi-panel results indicate that some built-environment elements such as outdoor spaciousness and mixed land use are causes of auto ownership (remaining even after attitudes were allowed to enter the model), but their effects are marginal. In contrast, the strong influence of sociodemographics suggests that households' auto-ownership decisions are fundamentally based on their mobility needs and purchasing power. Given the mixed findings, we do not definitively confirm a causal relationship between the built environment and auto ownership. However, we provide encour- aging evidence that land-use policies designed to reduce auto ownership and use will lead to a marginal reduction in auto ownership.},
annote = {From Duplicate 1 ( 








Cross-sectional and quasi-panel explorations of the connection between the built environment and auto ownership








- Cao, Xinyu; Mokhtarian, Patricia L; Handy, Susan L )


















From Duplicate 2 ( 








Cross-sectional and quasi-panel explorations of the connection between the built environment and auto ownership








- Cao, Xinyu; Mokhtarian, Patricia L; Handy, Susan L )







Cao et al. used quasi-panel data collected from a survey to evaluate the effects of attitudinal and built-environment characteristics on automobile ownership. The data are collected by mail-back surveys, using a residential advertising list as the sampling frame. The sampling strategy explicity separated traditional neighborhoods from suburban developments, and movers from non-movers. Respondents were asked questions regarding neighborhood characteristics, neighborhood preference, travel attitude, and sociodemographics. Scores are created for characteristics, preferences, and attitudes. The auto ownership of movers is also modeled with a static score model.




The authors report two ordered probit models on the cross-sectional data: one excluding travel attitudes and neighborhood preferences, and the other including them. Including attitudes and preferences significantly improves the model prediction, and also renders stated neighborhood characteristics immaterial. 




The simplifying reasons for reducing the survey responses to attribute scores are obvious, but I worry that information may be lost in the conversion. I am not familiar with the pattern matrix used to normalize these things (if that is in fact what it does), it nevertheless seems that the questions should have been incorporated into some model directly. The fact that stated preference variables supersede revealed preference variables in the model is similarly puzzling, but this could be simply a fact of individuals remaining unsatisfied with their current environments. 




The main contribution of this paper is that it quantifies the effect of residential self-selection bias, and yet leaves open the possibility for causal relationships between the built environment of a residential location and auto ownership.},
author = {Cao, Xinyu and Mokhtarian, Patricia L. and Handy, Susan L.},
doi = {10.1068/a37437},
file = {:Users/gregmacfarlane/Documents/Mendeley Desktop/2007/Cao, Mokhtarian, Handy{\_}2007{\_}Cross-sectional and quasi-panel explorations of the connection between the built environment and auto owners.pdf:pdf},
issn = {0308-518X},
journal = {Environment and Planning A},
number = {4},
pages = {830--847},
title = {{Cross-sectional and quasi-panel explorations of the connection between the built environment and auto ownership}},
url = {http://www.envplan.com/abstract.cgi?id=a37437},
volume = {39},
year = {2007}
}
