@article{Iacono2011,
abstract = {Regional location factors, with measures of regional accessibility foremost among them, 3 exert a strong influence on urban property markets. While accessibility represents an important 4 regional-scale factor, more local influences such as proximity to urban highway links may 5 also positively or negatively influence the desirability of a location. In this paper, we use a 6 cross-section of home sales in Hennepin County, Minnesota from the years 2001 through 2004, 7 along with a set of disaggregate regional accessibility measures, to estimate the value of access 8 to employment and resident workers. We also estimate the (dis)amenity effects of locations 9 near major freeway links that have recently undergone, or were scheduled to undergo (as of the 10 time period covered by the home sales), major construction to add capacity. The richness of the 11 home sales data set allows us to control for a number of structural attributes, as well as some 12 site characteristics, while additional neighborhood characteristics (such as income levels and 13 local educational quality) are added from supplemental data sources. Empirical results indicate 14 that households highly value employment access, while access to other resident workers (i.e. 15 competition for jobs) is considered a disamenity. Proximity to local highway access points is 16 positively associated with sale price, while proximity to the highway link itself is negatively 17 associated with price. The paper concludes with some implications for research and practice of 18 the concept and measurement of the relationship between location and land value.},
author = {Iacono, Michael and Levinson, David M.},
doi = {10.2139/ssrn.1736213},
