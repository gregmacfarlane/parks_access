file = {:Users/gregmacfarlane/Documents/Mendeley Desktop/2015/Macfarlane, Garrow, Moreno-Cruz{\_}2015{\_}Do Atlanta residents value MARTA Selecting an autoregressive model to recover willingness to pay.pdf:pdf},
journal = {Transportation Research Part A: Policy and Practice},
keywords = {land value capture,spatial Durbin model,spatial econometrics,transportation accessibility},
pages = {214--230},
title = {{Do Atlanta residents value MARTA? Selecting an autoregressive model to recover willingness to pay.}},
