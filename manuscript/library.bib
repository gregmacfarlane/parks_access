@article{Cervero1997,
annote = {This article looks at the land use impacts of the Bay Area Rapid Transit (BART) system roughly twenty years after its completions. Many studies had been conducted roughly 3-5 years following construction and argued that the BART system had minimal impacts on the land development outside the centre city, but this article looks at the situation farther down the road to see if that conclusion is still valid. These studies found that populations grew more significantly in communities not serviced by the system, employment opportunities also increased in communities not serviced by BART however, executive, professional and technical jobs increased significantly along BART routes over a 20 year period. They also found that downtown San Francisco experienced the most benefit from the system, while other communities did not see much impact on their demographics or built environment. This article is incredibly useful because it provides a perspective of the impacts of a system more than 20 years after the completion of construction. It is a perspective that will be useful when examining the possible impacts of a new transit system in York Region because the settings are somewhat comparable as suburban locations with low density and high reliability on vehicle travel. },
author = {Cervero, Robert and Landis, John},
doi = {10.1016/S0965-8564(96)00027-4},
issn = {09658564},
journal = {Transportation Research Part A: Policy and Practice},
month = {jul},
