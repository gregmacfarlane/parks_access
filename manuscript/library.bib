file = {:Users/gregmacfarlane/Documents/Mendeley Desktop/2014/Chandra, Vadali{\_}2014{\_}Evaluating accessibility impacts of the proposed America 2050 high-speed rail corridor for the Appalachian Region.pdf:pdf},
issn = {09666923},
journal = {Journal of Transport Geography},
keywords = {Accessibility,America 2050,Appalachian Region,High-speed rail,Manufacturing,Mining},
month = {may},
pages = {28--46},
title = {{Evaluating accessibility impacts of the proposed America 2050 high-speed rail corridor for the Appalachian Region}},
