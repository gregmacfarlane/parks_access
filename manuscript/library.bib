author = {Hadi, Mohammed A and Thakkar, Jan},
isbn = {03611981},
journal = {Transportation Research Record},
keywords = {Accident prevention,Computer simulation,Driveways,Highway systems,Highway traffic control,Mathematical models,Safety factor,Transportation routes},
number = {1847},
