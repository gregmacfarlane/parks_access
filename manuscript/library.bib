journal = {Regional Science and Urban Economics},
keywords = {Count model,Location choice,Manufacturing,Poisson,Spatial econometrics},
number = {4},
pages = {241--252},
publisher = {Elsevier B.V.},
title = {{A two-step estimator for a spatial lag model of counts: Theory, small sample performance and an application}},
url = {http://dx.doi.org/10.1016/j.regsciurbeco.2010.04.001},
volume = {40},
year = {2010}
}
@article{Friedrich2010,
abstract = {The core of this paper presents a method for generating O-D matrices by using floating phone data (FPD), that is, data of mobile phones moving through a study area. Mobile phones signals recorded in the cellular phone network are used to derive time-space trajectories of moving mobile phone devices. The start and end point of each trajec-tory determine the origin and destination zone. Then link counts are used to project the sample of mobile phone movements to the entire movements of cars, trucks, and rail passengers. Because travel patterns vary from day to day, O-D matrices are merged to typical traffic days, which are determined on the basis of clustered traffic counts from roadside detectors. This finally provides a set of O-D matrices of typical traffic days for planning purposes. The data set used in this paper was recorded in the cellular phone network of T-Mobile in a study area of 100 Ã 100 km in southwest Germany (Figure 1). It covers a time period of almost 70 days. The method uses data mainly from location-area updates, which are recorded from any mobile phone in standby mode, and thus does not require phone calls. This permits observing entire trips from their origin to their destination and not just trip segments covered by a single phone call. The signals produced by a particular mobile phone can be tracked because each phone possesses a unique anonymous identification (ID). This ID is modified every 24 h at midnight. Therefore it is not possible to track a single phone during a longer period. Considering these limitations, the method presented in this paper permits the generation of O-D matrices for â¢ Typical day types; â¢ Passenger car, truck, and passenger rail modes; and â¢ A zoning system in which the zone size complies with the location areas of the cellular phone network. METHODOLOGY The method for generating O-D matrices from mobile phone data uses three main data sources as input: â¢ Network model containing the relevant road and rail infrastructure and the position of count locations; â¢ Load curves of traffic volumes in 15-min intervals from count locations in the road network (cars, trucks) and in the rail network (passengers); and â¢ Trajectories from FPD-the method for generating such trajec-tories from FPD is described in Friedrich et al. (3) and Schlaich et al. (4). The spatial course of a trajectory is described by an origin zone, destination zone, and sequence of network links. Temporal information is available for each location update, that is, approximately every 5 to 15 min. A trajectory belongs to one mode of transport. This paper presents a method for generating origin-destination (O-D) matrices with the use of floating phone data, that is, data generated from mobile phones moving through a study area. Mobile phone signals recorded in the cellular phone network are used to derive time-space tra-jectories of moving mobile phone devices. The start and end points of each trajectory determine the origin and destination zone. Link counts are used to project the sample of mobile phone movements to the broader movement of cars, trucks, and rail passengers. With results of a clustering process of traffic counts, O-D matrices for typical traffic days are computed. The resulting O-D matrices can be used for a long-term traffic state forecast. The knowledge of travel demand is essential for planning and operating the transport supply. Origin-destination (O-D) matrices contain information on the travel demand in a study area in condensed form. Each matrix element quantifies the number of movements between an origin and a destination zone. The movements can refer to the aggregated travel demand of all travelers in a specific time interval or to more disaggregated levels in which the movements describe the travel demand of a particular mode, trip purpose, or person group. Travel surveys guaranteeing statistically ascertained values for the number of movements on the level of O-D pairs require a large sample size. For that reason and because it is difficult to observe all movements in a study area, O-D matrices are usually generated by using a travel demand model. In traditional travel demand modeling following the four-stage approach, O-D matrices result from the trip distribution and the mode choice step. The mode-specific O-D matrices then provide the input for the traffic assignment step. For calibrating the parameters of the trip distribution model it is common to compare modeled trip time distributions in the network with observed trip time distributions from household trip diary surveys. After the assignment, the modeled traffic volumes can be compared with observed traffic count volumes on the link level. Occasionally matrix correction or matrix update techniques are applied after assignment to produce a corrected travel demand matrix from a base matrix by using traffic count values. Willumsen (1) and Van Zuylen and Willumsen (2) describe such a method for a matrix correction based on the entropy maximization approach.},
author = {Friedrich, Markus and Immisch, Katrin and Jehlicka, Prokop and Otterst{\"{a}}tter, Thomas and Schlaich, Johannes},
doi = {10.3141/2196-10},
file = {:Users/gregmacfarlane/Documents/Mendeley Desktop/2010/Friedrich et al.{\_}2010{\_}Generating Origin-Destination Matrices from Mobile Phone Trajectories.pdf:pdf},
journal = {Transportation Research Record: Journal of the Transportation Research Board},
pages = {93--101},
title = {{Generating Origin-Destination Matrices from Mobile Phone Trajectories}},
url = {https://journals.sagepub.com/doi/pdf/10.3141/2196-10},
volume = {2196},
year = {2010}
}
@article{Willigers2011a,
abstract = {High-speed rail is seen as a factor contributing to the attractiveness of a location for economic activities. This paper focuses on how the level-of-service characteristics of railway stations, and in particular the presence of high-speed train services, influence the attractiveness of locations for specific types of offices. The results are presented for a stated choice experiment for location choices of offices in the Netherlands. It is concluded that the availability of high-speed train services contributes to the attractiveness of a location for offices. For internationally-oriented offices the areas around stations with international high-speed train services are attractive because of their good international accessibility. We also found an indication that high-speed train services can raise the status of an office site. In the Netherlands, the domestic high-speed train services are less relevant for location choices, because of the small domestic distances. Besides high-speed train services, other location characteristics that determine how well a site is connected to the railway network are also found to be important for location choices. Thereby differences between offices occur, which can partly be explained by the number of trips to/from an office.},
annote = {A stated preference survey of firm location decisions with relation to the HSR lines in Holland.},
author = {Willigers, Jasper and van Wee, Bert},
doi = {10.1016/j.jtrangeo.2010.09.002},
file = {:Users/gregmacfarlane/Documents/Mendeley Desktop/2011/Willigers, van Wee{\_}2011{\_}High-speed rail and office location choices. A stated choice experiment for the Netherlands.pdf:pdf},
issn = {09666923},
journal = {Journal of Transport Geography},
keywords = {Accessibility,Firms,HSR,HST,Location choice,Office location,spatial location},
mendeley-tags = {HSR,spatial location},
month = {jul},
